{
    "name": "Mocking-Bird",
    "version": "0.1.0",
    "description": "Delivers fresh tweet from your selection of sources to your new tab",
    "manifest_version": 2,

    "content_security_policy": "script-src 'self' ./; object-src 'self'",

    "permissions": [
        "activeTab",
        "storage"
    ],

    "chrome_url_overrides": {
        "newtab": "mocking-bird.html"
    }

}
