{
    "name": "Mocking-Bird",
    "version": "0.1.0",
    "description": "Delivers fresh tweet from your selection of sources to your new tab",
    "manifest_version": 2,

    "content_security_policy": "script-src 'self' 'sha256-NbDluRbaqbYMpKCaxSZSnxED47GRvXT/oxeiHOt6aoM=' 'sha256-QxmouP/PuhFtzBOU2BXetWY8O2ZWvH2Xq9NyIAvUjUU=' 'sha256-jEMyb+HjdsnDDpa2FnospymZgkWxlfix8f0M6QdLUtE=' https://platform.twitter.com; object-src 'self'",

    "page_action": {
        "default_icon": "mocking-bird-final.png"
    },

    "options_page": "options.html",

    "permissions": [
        "storage"
    ],

    "chrome_url_overrides": {
        "newtab": "mocking-bird.html"
    }

}
